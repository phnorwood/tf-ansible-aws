# Copy this file to terraform.tfvars and fill in your values.
# terraform.tfvars is gitignored — never commit real keys.

aws_region = "us-east-1"

key_name = "tf-ansible-key"

# Username that Ansible will create on the instance
managed_user = "deploy"

# Your personal public key — installed for the managed user so you can SSH in
# after Ansible runs. Paste the contents of e.g. ~/.ssh/id_ed25519.pub
managed_user_public_key = "ssh-ed25519 AAAA... user@host"

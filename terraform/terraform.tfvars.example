# Copy this file to terraform.tfvars and fill in your values.
# terraform.tfvars is gitignored â€” never commit real keys.

aws_region = "us-east-1"

key_name = "tf-ansible-key"

# Paste the full contents of your public key file, e.g. ~/.ssh/id_ed25519.pub
ec2_public_key = "ssh-ed25519 AAAA... user@host"

# Username that Ansible will create on the instance
managed_user = "deploy"

# Public key for the managed user (can be the same as ec2_public_key)
managed_user_public_key = "ssh-ed25519 AAAA... user@host"

# Absolute or ~ path to the matching private key (used by Ansible)
ansible_private_key_path = "~/.ssh/id_ed25519"
